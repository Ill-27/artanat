{
  "routes": [
    {
      "src": "/api/(bg1|btw)",
      "headers": {
        "Content-Security-Policy": "frame-ancestors https://arterrii.ru https://arterrii.vercel.app;",
        "Access-Control-Allow-Origin": "https://arterrii.vercel.app",
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "Authorization",
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; frame-ancestors https://arterrii.vercel.app https://arterrii.ru;",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Cache-Control": "no-store"
      }
    }
  ]
}
